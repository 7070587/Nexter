{
    "name": "nexter",
    "version": "1.00.00",
    "description": "Landing page for nexter",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel live watch",
        "live": "live-server --browser=firefox",
        "watch": "sass --watch scss/main.scss:css/style.css",
        "compile": "sass scss/main.scss:css/style.css",
        "prefix": " postcss css/style.css --replace --use autoprefixer",
        "compress": "sass scss/main.scss:css/style.css --style=compressed",
        "build": "sass scss/main.scss:css/style.css --style=compressed && postcss css/style.css --replace --use autoprefixer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/7070587/Nexter"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/7070587/Nexter/issues"
    },
    "homepage": "https://github.com/7070587/Nexter#readme",
    
    "devDependencies": {
        "autoprefixer": "^10.2.4",
        "concat": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.5",
        "postcss-cli": "^8.3.1",
        "sass": "^1.32.8",
        "live-server": "^1.2.1"
    },
    "dependencies": {}
}
